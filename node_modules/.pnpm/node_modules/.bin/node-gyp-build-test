#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules/.pnpm/node-gyp-build@4.3.0/node_modules/node-gyp-build/node_modules:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules/.pnpm/node-gyp-build@4.3.0/node_modules:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules/.pnpm/node_modules:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules:/Users/urielblanco/Documents/Development/node_modules:/Users/urielblanco/Documents/node_modules:/Users/urielblanco/node_modules:/Users/node_modules:/node_modules:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules/.pnpm/node_modules/node-gyp-build/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules/.pnpm/node-gyp-build@4.3.0/node_modules/node-gyp-build/node_modules:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules/.pnpm/node-gyp-build@4.3.0/node_modules:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules/.pnpm/node_modules:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules:/Users/urielblanco/Documents/Development/node_modules:/Users/urielblanco/Documents/node_modules:/Users/urielblanco/node_modules:/Users/node_modules:/node_modules:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules/.pnpm/node_modules/node-gyp-build/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-gyp-build/build-test.js" "$@"
else
  exec node  "$basedir/../node-gyp-build/build-test.js" "$@"
fi
