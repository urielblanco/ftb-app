#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules/.pnpm/network-address@1.1.2/node_modules/network-address/node_modules:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules/.pnpm/network-address@1.1.2/node_modules:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules/.pnpm/node_modules:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules:/Users/urielblanco/Documents/Development/node_modules:/Users/urielblanco/Documents/node_modules:/Users/urielblanco/node_modules:/Users/node_modules:/node_modules:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules/.pnpm/node_modules/network-address/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules/.pnpm/network-address@1.1.2/node_modules/network-address/node_modules:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules/.pnpm/network-address@1.1.2/node_modules:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules/.pnpm/node_modules:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules:/Users/urielblanco/Documents/Development/node_modules:/Users/urielblanco/Documents/node_modules:/Users/urielblanco/node_modules:/Users/node_modules:/node_modules:/Users/urielblanco/Documents/Development/blockchain-tfm/node_modules/.pnpm/node_modules/network-address/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../network-address/cli.js" "$@"
else
  exec node  "$basedir/../network-address/cli.js" "$@"
fi
